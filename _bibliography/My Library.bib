  keywords = {Computer Science - Artificial Intelligence,Computer Science - Computer Vision and Pattern Recognition,Computer Science - Machine Learning},
  file = {/Users/samhoover/Zotero/storage/6XB2YXLS/Alayrac et al. - 2022 - Flamingo a Visual Language Model for Few-Shot Lea.pdf;/Users/samhoover/Zotero/storage/VGRMAZ3I/2204.html}
