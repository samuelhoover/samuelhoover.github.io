}

@article{yuUnidirectionalSinglefileTransport2023,
  title = {Unidirectional Single-File Transport of Full-Length Proteins through a Nanopore},
  author = {Yu, Luning and Kang, Xinqi and Li, Fanjun and Mehrafrooz, Behzad and Makhamreh, Amr and Fallahi, Ali and Foster, Joshua C. and Aksimentiev, Aleksei and Chen, Min and Wanunu, Meni},
