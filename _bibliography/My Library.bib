@article{manghiPhysicsBasepairingDynamics2016,
  title = {Physics of Base-Pairing Dynamics in {{DNA}}},
  author = {Manghi, Manoel and Destainville, Nicolas},
  year = {2016},
  journal = {Physics Reports},
  series = {Physics of Base-Pairing Dynamics in {{DNA}}},
  volume = {631},
