@article{bakshlooPolypeptideAnalysisNanoporebased2022,
  title = {Polypeptide Analysis for Nanopore-Based Protein Identification},
  author = {Bakshloo, Mazdak Afshar and Yahiaoui, Safia and Piguet, Fabien and {Pastoriza-Gallego}, Manuela and Daniel, R{\'e}gis and Math{\'e}, J{\'e}r{\^o}me and Kasianowicz, John J. and Oukhaled, Abdelghani},
