@article{jungPhaseBehaviorContinuousspace2020,
  title = {Phase Behavior of Continuous-Space Systems: {{A}} Supervised Machine Learning Approach},
  author = {Jung, Hyuntae and Yethiraj, Arun},
  year = {2020},
