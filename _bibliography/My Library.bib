@article{nikolaevAutonomyMaterialsResearch2016,
  title = {Autonomy in Materials Research: {{A}} Case Study in Carbon Nanotube Growth},
  author = {Nikolaev, Pavel and Hooper, Daylond and Webber, Frederick and Rao, Rahul and Decker, Kevin and Krein, Michael and Poleski, Jason and Barto, Rick and Maruyama, Benji},
