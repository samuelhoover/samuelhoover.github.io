@article{yongAcceleratingLangevinFieldTheoretic2022,
  title = {Accelerating {{Langevin Field-Theoretic Simulation}} of {{Polymers}} with {{Deep Learning}}},
  author = {Yong, Daeseong and Kim, Jaeup U.},
