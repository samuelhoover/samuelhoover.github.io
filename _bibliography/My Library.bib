@article{kesimerAnalyzingFunctionsLarge2008a,
  title = {Analyzing the Functions of Large Glycoconjugates through the Dissipative Properties of Their Absorbed Layers Using the Gel-Forming Mucin {{MUC5B}} as an Example},
