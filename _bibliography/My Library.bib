@article{schwarzeCompleteCostsGenome2020,
  title = {The Complete Costs of Genome Sequencing: A Microcosting Study in Cancer and Rare Diseases from a Single Center in the {{United Kingdom}}},
  author = {Schwarze, Katharina and Buchanan, James and Fermont, Jilles M. and Dreau, Helene and Tilley, Mark W. and Taylor, John M. and Antoniou, Pavlos and Knight, Samantha J.L. and Camps, Carme and Pentony, Melissa M. and Kvikstad, Erika M. and Harris, Steve and Popitsch, Niko and Pagnamenta, Alistair T. and Schuh, Anna and Taylor, Jenny C. and Wordsworth, Sarah},
