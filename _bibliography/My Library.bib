@article{perry100thAnniversaryMacromolecular2020,
  title = {100th {{Anniversary}} of {{Macromolecular Science Viewpoint}}: {{Opportunities}} in the {{Physics}} of {{Sequence-Defined Polymers}}},
  shorttitle = {100th {{Anniversary}} of {{Macromolecular Science Viewpoint}}},
  author = {Perry, Sarah L. and Sing, Charles E.},
