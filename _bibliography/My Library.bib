@article{mahalikLangevinDynamicsSimulation2013,
  title = {Langevin Dynamics Simulation of {{DNA}} Ejection from a Phage},
  author = {Mahalik, J. P. and Hildebrandt, B. and Muthukumar, Murugappan},
  year = {2013},
  journal = {Journal of Biological Physics},
  volume = {39},
