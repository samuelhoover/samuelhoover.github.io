@article{zhouExtraordinaryTemperatureDependence2022,
  title = {Extraordinary {{Temperature Dependence}} of {{Hierarchically Assembled Macromolecular Structures}} with {{Memory}}},
  author = {Zhou, Chao and Wang, Yubin and Muthukumar, Murugappan and Zhang, Ruoyu and Zhao, Jiang and Jia, Di},
  year = {2022},
