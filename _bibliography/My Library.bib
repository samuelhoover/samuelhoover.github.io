  file = {/Users/samhoover/Zotero/storage/5TF4RMC4/Khokhlov and Kramarenko - 1994 - PolyelectrolyteIonomer behavior in polymer gel co.pdf;/Users/samhoover/Zotero/storage/6Q72AAP2/mats.1994.html}
}

